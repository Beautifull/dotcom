create table mradwin_mt1.hebcal_shabbat_email (
    email_verify varchar(16) not null primary key,
    email_address varchar(255) not null,
    email_status varchar(16) not null,
    email_created timestamp not null,
    email_confirmed timestamp,
    email_candles_zip integer(5),
    email_candles_timezone varchar(8),
    email_candles_dst varchar(8),
    email_candles_havdalah tinyint,
    email_candles_city varchar(40),
    email_optin_announce tinyint(1),
    unique (email_address),
    index (email_status)
);

create table mradwin_mt1.hebcal_zips (
    zips_zipcode integer(5) not null primary key,
    zips_latitude varchar(12) not null,
    zips_longitude varchar(12) not null,
    zips_timezone varchar(8) not null,
    zips_dst tinyint(1) not null,
    zips_city varchar(64) not null,
    zips_state varchar(2) not null
);
